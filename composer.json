{
    "name": "ichbin/filament-forum",
    "description": "This is my package filament-forum",
    "keywords": [
        "IchBin",
        "laravel",
        "filament-forum"
    ],
    "version": "1.0.0",
    "homepage": "https://github.com/ichbin/filament-forum",
    "support": {
        "issues": "https://github.com/ichbin/filament-forum/issues",
        "source": "https://github.com/ichbin/filament-forum"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "IchBin",
            "email": "contact@gametracker.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "filament/upgrade": "3.0-stable"
    },
    "autoload": {
        "psr-4": {
            "IchBin\\FilamentForum\\": "src/",
            "IchBin\\FilamentForum\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IchBin\\FilamentForum\\Tests\\": "tests/"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IchBin\\FilamentForum\\FilamentForumServiceProvider"
            ],
            "aliases": {
                "FilamentForum": "IchBin\\FilamentForum\\Facades\\FilamentForum"
            }
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
